dict = {'CU': 'see you', ':-)': "I'm happy", ':-(' : "I'm unhappy", ';-)' : 'wink', ':-P' : 'stick out my tongue', '(~.~)' : 'sleepy', 'TA' : 'totally awesome', 'CCC' : 'Canadian Computing Competition', 'CUZ' : 'because', 'TY' : 'thank-you', 'YW' : "you're welcome", 'TTYL' : 'talk to you later'
};

lst = []
word = 'TTYL'
while True:
  user = input()
  lst.append(user)
  if word in user:
    break
    
for word in lst:
  if word in dict:
    print(dict[word])
  else:
    print(word)
